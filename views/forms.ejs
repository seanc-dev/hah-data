<!DOCTYPE html>
<html lang="en">

<head>
    <!-- <base href="/"> -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="/css/styles.css">
    <!-- <link rel="stylesheet" href="public/stylesheets/styles.css"> -->
    <title>Hire a Hubby <%= businessName %> Form Central</title>
</head>

<body>

    <div id="businessName" class="d-none"><%= businessName %></div>

    <div id="loadingOverlay">
            <div id="loadingMessage">
                <div id="loader">
                        <div class="lds-default"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div>
                </div>
            </div>
        </div>
        <script src="/js/loading-overlay.js"></script>

    <div class="container my-3">
        <div class="jumbotron">
            <h1 class="display-4 text-center">
                Hire a Hubby Kapiti Forms
            </h1>
        </div>
        <!-- tab row -->
        <section class="" id="appBody">
            <div id="tabRow" class="row text-center justify-content-center">
                <div class="col-6 tab-col">
                    <div class="card tab-card active-tab" data-tab-id="clientDetails">
                        <div class="card-body">
                            <!-- <i class="fas fa-user-circle"></i> -->
                            <i class="fas fa-portrait"></i>
                            <h4><span>New Client</span></h4>
                        </div>
                    </div>
                </div>
                <div class="col-6 tab-col">
                    <div class="card tab-card" data-tab-id="jobDetails">
                        <div class="card-body">
                            <i class="fas fa-search-plus"></i>
                            <h4><span>Job Details</span></h4>
                        </div>
                    </div>
                </div>
                <!-- <div class="col-4 tab-col" data-tab-id="staffDetails">
                    <div class="card tab-card">
                        <div class="card-body">
                            <i class="fas fa-rocket"></i>
                            <h4><span>Staff Rates</span></h4>
                        </div>
                    </div>
                </div> -->
            </div>
            <div id="contentBody">
                <div id="clientDetails" class="form-content row">
                    <div class="col-12 px-0">
                        <div class="card">
                            <div class="card-body">
                                <div class="error-message-row row px-0 d-none">
                                    <div class="col-12 px-0">
                                        <div class="card border-0">
                                            <div class="card-body px-0">
                                                <span class="error-message"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <form id="clientDetailsForm" class="" action="" method="get" accept-charset="utf-8">
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="jobDetails" class="form-content row d-none">
                    <div class="col-12 px-0">
                        <div class="card">
                            <div class="card-body">
                                <div class="error-message-row row px-0 d-none">
                                    <div class="col-12 px-0">
                                        <div class="card border-0">
                                            <div class="card-body px-0">
                                                <span class="error-message"></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <form id="jobDetailsForm" class="" action="" method="get" accept-charset="utf-8">
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- <div id="staffDetails" class="form-content row">
                    <div class="col-12 px-0">
                        <div class="card">
                            <div class="card-body">
                                <form id="staffDetailsForm" class="" action="http://wilder.azurewebsites.net/echo"
                                    method="get" accept-charset="utf-8">
                                </form>
                            </div>
                        </div>
                    </div>
                </div> -->
            </div>
        </section>

    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/axios/0.19.0/axios.min.js" integrity="sha256-S1J4GVHHDMiirir9qsXWc8ZWw74PHHafpsHp5PXtjTs=" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/js/bootstrap.min.js" type="text/javascript" charset="utf-8" async defer></script>
    <script type="module" src="/js/form-options.js" type="text/javascript" charset="utf-8" async defer></script>
    <script type="module" src="/js/forms.js" type="text/javascript" charset="utf-8" async defer></script>
    <script type="module" src="/js/handlers.js" type="text/javascript" charset="utf-8" async defer></script>
    <script type="module" src="/js/index.js" type="text/javascript" charset="utf-8" async defer></script>
</body>

</html>